{
  "name": "frontend",
  "version": "1.0.0",
  "scripts": {
    "build": "(set NODE_OPTIONS=--openssl-legacy-provider) && webpack --mode production",
    "build:dev": "(set NODE_OPTIONS=--openssl-legacy-provider) && webpack --mode development",
    "build:start": "cd dist && PORT=8080 npx serve",
    "start": "webpack serve --mode development",
    "start:live": "(set NODE_OPTIONS=--openssl-legacy-provider) && webpack serve --mode development --live-reload --hot"
  },
  "license": "MIT",
  "author": {
    "name": "Jack Herrington",
    "email": "jherr@pobox.com"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@playwright/test": "^1.45.0",
    "@types/node": "^20.14.9",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^6.3.0",
    "dotenv": "^16.0.2",
    "dotenv-webpack": "^8.0.1",
    "html-webpack-plugin": "^5.3.2",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "style-loader": "^3.3.0",
    "wait-port": "^1.1.0",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "babel-loader": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^1.0.1"
  }
}
